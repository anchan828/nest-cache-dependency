{
  "name": "@anchan828/nest-cache-packages",
  "private": true,
  "version": "0.0.0",
  "description": "The cache module for [Nest](https://github.com/nestjs/nest).",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "postinstall": "husky install && npm run bootstrap && npm run build",
    "bootstrap": "lerna bootstrap",
    "build": "lerna run build",
    "format": "prettier --write '**/*.{js,json,yml,yaml,md}'",
    "lint": "lerna run --parallel lint",
    "lint:fix": "lerna run --parallel lint:fix",
    "publish": "lerna publish --yes patch",
    "publish:minor": "lerna publish --yes minor",
    "publish:major": "lerna publish --yes major",
    "test": "lerna run test && npx @anchan828/merge-coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/anchan828/nest-cache-dependency.git"
  },
  "author": "anchan828 <anchan828@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/anchan828/nest-cache-dependency/issues"
  },
  "homepage": "https://github.com/anchan828/nest-cache-dependency#readme",
  "devDependencies": {
    "@commitlint/cli": "16.2.1",
    "@commitlint/config-conventional": "16.2.1",
    "@nestjs/common": "8.3.1",
    "@nestjs/core": "8.3.1",
    "@nestjs/platform-express": "8.3.1",
    "@nestjs/testing": "8.3.1",
    "@types/jest": "27.4.1",
    "@types/lru-cache": "7.4.0",
    "@types/node": "16.11.26",
    "@types/supertest": "2.0.11",
    "@typescript-eslint/eslint-plugin": "5.12.1",
    "@typescript-eslint/parser": "5.12.1",
    "cache-manager": "3.6.0",
    "eslint": "8.10.0",
    "eslint-config-prettier": "8.4.0",
    "eslint-plugin-prettier": "4.0.0",
    "husky": "7.0.4",
    "lru-cache": "7.4.0",
    "jest": "27.5.1",
    "lerna": "4.0.0",
    "lint-staged": "12.3.4",
    "prettier": "2.5.1",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.5.4",
    "supertest": "6.2.2",
    "ts-jest": "27.1.3",
    "ts-node": "10.5.0",
    "typescript": "4.5.5",
    "ulid": "2.3.0"
  }
}
