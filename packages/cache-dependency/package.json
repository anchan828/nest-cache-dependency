  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "copy:license": "cp ../../LICENSE ./",
    "copy:readme": "cp ../../README.md ./",
    "lint": "TIMING=1 eslint --ignore-path ../../.eslintignore '**/*.ts'",
